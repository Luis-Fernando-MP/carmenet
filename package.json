{
  "name": "JuliChat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "turbo": "rimraf .next && pnpm store prune && pnpm dev",
    "prisma:init": "pnpx prisma init --datasource-provider mysql",
    "prisma:migrate": "pnpx prisma migrate dev --name init"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.16.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/typescript-estree": "8.2.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "postcss": "8.4.41",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "rimraf": "6.0.1",
    "tailwindcss": "3.4.10",
    "ts-standard": "12.0.2",
    "typescript": "5.5.4"
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.json"
    ]
  }
}
